VERSION 120

EDIT UG_GATEWAY_MAIN_MENUBAR

BEFORE UG_HELP
	CASCADE_BUTTON KFSWD
	LABEL SidewallDesign
END_OF_BEFORE

MENU KFSWD
	
	BUTTON new_swd_contour
	LABEL Contour
	MESSAGE Loads or swaps a contour as a basis for a sidewall.
	ACTIONS swd_contour.dfa
	
	
	BUTTON new_swd_text
	LABEL Text Editor
	MESSAGE Adds text to a sidewall.
	ACTIONS swd_text.dfa
	
	BUTTON new_swd_Font
	LABEL Font Editor
	MESSAGE Creates or modifies a font.
	ACTIONS swd_font.dfa
	
END_OF_MENU
